{
    "name": "vscode-pastemyst",
    "displayName": "vscode-pastemyst",
    "description": "a thing",
    "publisher": "mellobacon",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.55.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onView:pastemyst.viewActions",
        "onView:pastemyst.viewExplorer",
        "onCommand:pastemyst.viewPaste"
    ],
    "main": "./dist/extension.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/mellobacon/vscode-pastemyst"
    },
    "contributes": {
        "views": {
            "pastemyst": [
                {
                    "id": "pastemyst.viewActions",
                    "name": "PasteMyst Actions"
                },
                {
                    "id": "pastemyst.viewExplorer",
                    "name": "PasteMyst Viewer"
                }
            ]
        },
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "pastemyst",
                    "icon": "resources/pastemyst.svg",
                    "title": "PasteMyst"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run package",
        "compile": "webpack",
        "watch": "webpack --watch",
        "package": "webpack --mode production --devtool hidden-source-map",
        "test-compile": "tsc -p ./",
        "test-watch": "tsc -watch -p ./",
        "pretest": "npm run test-compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.55.0",
        "@types/glob": "^7.1.3",
        "@types/node": "^12.11.7",
        "eslint": "^7.19.0",
        "@typescript-eslint/eslint-plugin": "^4.14.1",
        "@typescript-eslint/parser": "^4.14.1",
        "glob": "^7.1.6",
        "typescript": "^4.1.3",
        "ts-loader": "^8.0.14",
        "webpack": "^5.19.0",
        "webpack-cli": "^4.4.0"
    }
}
